                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT DISTINCT 
                      librarysetupinformation.institutename, librarysetupinformation.libraryname, librarysetupinformation.address, librarysetupinformation.city, 
                      librarysetupinformation.pincode, librarysetupinformation.state, librarysetupinformation.phoneno, librarysetupinformation.fax, 
                      librarysetupinformation.email, librarysetupinformation.gram, CircIssueMaster.userid, CircIssueMaster.currentissuedbooks, 
                      CircUserManagement.firstname + ' ' + CircUserManagement.middlename + ' ' + CircUserManagement.lastname AS firstname
FROM         AcedemicSessionInformation INNER JOIN
                      librarysetupinformation ON AcedemicSessionInformation.AcademicSession = librarysetupinformation.CurrentAcademicSession INNER JOIN
                      CircUserManagement INNER JOIN
                      CircIssueMaster ON CircUserManagement.userid = CircIssueMaster.userid INNER JOIN
                      CircIssueTransaction ON CircIssueMaster.userid = CircIssueTransaction.userid ON 
                      AcedemicSessionInformation.StartDate &lt; CircIssueTransaction.issuedate AND 
                      AcedemicSessionInformation.Enddate &gt; CircIssueTransaction.issuedate
ORDER BY CircIssueMaster.currentissuedbooks DESC</CommandText>
