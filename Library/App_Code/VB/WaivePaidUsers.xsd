                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT librarysetupinformation.institutename,librarysetupinformation.libraryname, librarysetupinformation.address, 
librarysetupinformation.city, librarysetupinformation.pincode, librarysetupinformation.state, 
librarysetupinformation.phoneno, librarysetupinformation.fax,librarysetupinformation.email, librarysetupinformation.gram, 
librarysetupinformation.currency,
waivepaid.userid, CircUserManagement.firstname+' '+middlename+' '+lastname as memname, 
institutemaster.Shortname + '-' + departmentmaster.departmentname as deptname, 
CircUserManagement.classname, waivepaid.waivedate, waivepaid.totaloverdue,  
waivepaid.paidoverdue, waivepaid.reason 
FROM waivepaid inner join circusermanagement on waivepaid.userid=CircUserManagement.userid 
inner join departmentmaster on circusermanagement.departmentcode=departmentmaster.departmentcode 
INNER JOIN institutemaster on departmentmaster.institutecode=institutemaster.institutecode
CROSS JOIN Librarysetupinformation</CommandText>
