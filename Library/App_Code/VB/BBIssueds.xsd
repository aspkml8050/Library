                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT     CircIssueTransaction.userid, CircIssueTransaction.accno, CircIssueTransaction.BBSecurity, 
                      CircUserManagement.firstname + ' ' + CircUserManagement.middlename + ' ' + CircUserManagement.lastname AS memname, 
                      Program_Master.program_name AS course, GETDATE() AS issuedate, BookCatalog.title, 
                      BookAuthor.firstname1 + ' ' + BookAuthor.middlename1 + ' ' + BookAuthor.lastname1 + ' ' + BookAuthor.firstname2 + ' ' + BookAuthor.middlename2 + ' ' +
                       BookAuthor.lastname2 + ' ' + BookAuthor.firstname3 + ' ' + BookAuthor.middlename3 + ' ' + BookAuthor.lastname3 AS Author, 
                      bookaccessionmaster.bookprice, '' AS recieptno, librarysetupinformation.institutename, librarysetupinformation.libraryname, 
                      librarysetupinformation.address, librarysetupinformation.city, librarysetupinformation.pincode, librarysetupinformation.state, 
                      librarysetupinformation.phoneno, librarysetupinformation.fax, librarysetupinformation.email, librarysetupinformation.gram, 
                      librarysetupinformation.currency, librarysetupinformation.CurrentAcademicSession, librarysetupinformation.BBSecurity AS BBSecurityPercentage
FROM         CircIssueTransaction INNER JOIN
                      CircUserManagement ON CircIssueTransaction.userid = CircUserManagement.userid INNER JOIN
                      Program_Master ON CircUserManagement.program_id = Program_Master.program_id INNER JOIN
                      bookaccessionmaster ON CircIssueTransaction.accno = bookaccessionmaster.accessionnumber INNER JOIN
                      BookCatalog ON bookaccessionmaster.ctrl_no = BookCatalog.ctrl_no INNER JOIN
                      BookAuthor ON bookaccessionmaster.ctrl_no = BookAuthor.ctrl_no CROSS JOIN
                      librarysetupinformation
WHERE     (CircIssueTransaction.BBSecurity IS NOT NULL) OR
                      (CircIssueTransaction.BBSecurity &gt; 0)</CommandText>
