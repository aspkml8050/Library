                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>SELECT     librarysetupinformation.institutename, librarysetupinformation.libraryname, librarysetupinformation.address, librarysetupinformation.city, 
                      librarysetupinformation.pincode, librarysetupinformation.state, librarysetupinformation.phoneno, librarysetupinformation.fax, 
                      librarysetupinformation.email, librarysetupinformation.gram, Fine_calculation.userid, SUM(Fine_calculation.fine) AS fine, 
                      CircUserManagement.firstname, librarysetupinformation.currency
FROM         Fine_calculation INNER JOIN
                      CircUserManagement ON CircUserManagement.userid = Fine_calculation.userid CROSS JOIN
                      librarysetupinformation
